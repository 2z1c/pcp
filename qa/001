#!/bin/sh
# PCP QA Test No. 001
# exercise the PDU encode/decode ... no PMCD involvement
#
# Copyright (c) 1995-2002 Silicon Graphics, Inc.  All Rights Reserved.
#

seq=`basename $0`
echo "QA output created by $seq"

# get standard filters
. ./common.product
. ./common.filter

trap "rm -f $tmp.*; exit" 0 1 2 3 15

src/pducheck

# If you add stuff here, it will make the test fail but this
# is a good place to mine the github CI environment for stuff
# we need to know about ...
#
echo "github CI mining ... this is NOT a qa/001 failure"
env
for dir in /sys /sys/block
do
    echo "=== $dir ==="
    if [ -d "$dir" ]
    then
	ls -l "$dir"
    else
	echo "Not a directory!"
    fi
done
